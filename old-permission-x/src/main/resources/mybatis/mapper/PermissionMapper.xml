<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.old.ssm.mapper.PermissionMapper">

	<select id="findById" resultType="org.old.ssm.entity.Permission">
		select * from t_permission where id=#{id}
	</select>
	
	<select id="findAll" resultType="org.old.ssm.entity.Permission">
		select * from t_permission
	</select>

</mapper>